					$('#Kff'+num+'').removeClass('Kactive_tan');
					$('#Kff'+num+'').addClass('Kactive_ta');
				});

				$('.map').click(function(){
					if($(this).hasClass('hid')){$(this).removeClass('hid');}else{$(this).toggleClass('hid');};
				});
		});
